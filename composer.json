{
  "name": "paulocoelho/laravel-scaffold-plus",
  "description": "Geração completa de estruturas Laravel: Model, Controller, Service, Repository, Provider, Requests e Policy com exemplos de uso.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PauloCoelho\\ScaffoldPlus\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PauloCoelho\\ScaffoldPlus\\ScaffoldServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.4",
    "illuminate/support": "^v12.31.1",
    "illuminate/console": "^v12.32.5",
    "illuminate/filesystem": "^v12.32.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3.11"
  }
}